{
  "name": "lint-to-the-future",
  "version": "2.1.0",
  "description": "A modern way to progressively update your code to the best practices",
  "repository": "https://github.com/mansona/lint-to-the-future",
  "license": "MIT",
  "author": "Chris Manson <chris@manson.ie>",
  "bin": {
    "lint-to-the-future": "cli.js",
    "lttf": "cli.js"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "cli.js",
    "dist"
  ],
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:node": "mocha node-tests/*",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "dependencies": {
    "chai": "^4.3.6",
    "commander": "^9.4.1",
    "fs-extra": "^7.0.1",
    "import-cwd": "^3.0.0",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.1.0",
    "@ember/test-helpers": "^2.7.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "cheerio": "^1.0.0-rc.12",
    "ember-auto-import": "^2.4.1",
    "ember-body-class": "^3.0.0",
    "ember-cli": "~4.4.1",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-data": "~4.4.0",
    "ember-export-application-global": "^2.0.1",
    "ember-fetch": "^8.1.1",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.4.0",
    "ember-template-lint": "^4.8.0",
    "ember-test-selectors": "^7.0.0",
    "ember-welcome-page": "^6.2.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "execa": "^7.0.0",
    "express": "^4.17.1",
    "fixturify-project": "^7.1.2",
    "frappe-charts": "^1.5.2",
    "glob": "^4.5.3",
    "lint-to-the-future-eslint": "^2.0.1",
    "loader.js": "^4.7.0",
    "mocha": "^10.1.0",
    "morgan": "^1.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "qunit": "^2.19.1",
    "qunit-dom": "^2.0.0",
    "release-plan": "^0.9.0",
    "temp": "^0.9.4",
    "webpack": "^5.72.1"
  },
  "packageManager": "pnpm@7.33.7+sha512.7afe2410642b39c698df46da4ce5c7231a5cce360698d69f8cf8f42257d40b3e43f231053b07b8de849fd4ffbf4a71ff57b835137777a352388f3d3da747200e",
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "pnpm": {
    "overrides": {
      "undici": "^5.0.0",
      "whatwg-url": "^14.0.0"
    },
    "patchedDependencies": {
      "fixturify-project@7.1.2": "patches/fixturify-project@7.1.2.patch"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "paths": [
      "lib/testem-middleware"
    ]
  }
}
